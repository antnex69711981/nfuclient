{
    "name": "antnex",
    "namespace": "antnex",
    "version": "2023111502",
    "framework": "ext",
    "bypass_lp": true,

    "requires": [
        "font-awesome",
        "locale",
        "charts"
    ],

    "locale": "zh_TW",

    "indexHtmlPath": "index.html",

    "classpath": [
        "app",
        "${toolkit.name}/src"
    ],

    "overrides": [
        "overrides",
        "${toolkit.name}/overrides"
    ],


    "fashion": {
        "missingParameters": "error",
        "inliner": {
            "enable": false
        }
    },

    "sass": {
        "namespace": "antnex",
        "etc": [
            "sass/etc/all.scss",
            "${toolkit.name}/sass/etc/all.scss"
        ],
        "var": [
            "sass/var/all.scss",
            "sass/var",
            "${toolkit.name}/sass/var/all.scss",
            "${toolkit.name}/sass/var"
        ],
        "src": [
            "sass/src",
            "${toolkit.name}/sass/src"
        ]
    },

    "js": [
        {
            "path": "lib/log4js-mini.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "lib/initUtil.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "lib/rpt2.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "node_modules/string/dist/string.min.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "node_modules/underscore/underscore-min.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "node_modules/numeral/min/numeral.min.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "node_modules/moment/min/moment-with-locales.min.js",
            "update": "full",
            "includeInBundle": true
        },
        // {
        //     "path": "node_modules/hashids/dist/hashids.min.js",
        //     "update": "full",
        //     "includeInBundle": true
        // },
        {
            "path": "node_modules/noty/lib/noty.min.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "node_modules/crypto-js/crypto-js.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "node_modules/xlsx/dist/xlsx.full.min.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path":"node_modules/suneditor/dist/suneditor.min.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path":"lib/codemirror.min.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path":"lib/css.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path":"lib/htmlmixed.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path":"lib/xml.js",
            "update": "full",
            "includeInBundle": true
        },
        {
            "path": "app.js",
            "bundle": true
        }
    ],

    "classic": {
        "js": [
            {
                "path": "${framework.dir}/build/ext-all-rtl-debug.js"
            }
        ]
    },

    "modern": {
        "js": [
            {
                "path": "${framework.dir}/build/ext-modern-all-debug.js"
            }
        ]
    },

    "css": [
        {
            "path": "resources/antnex.css",
            "update": ""
        },
        {
            "path": "resources/ux/ux.css",
            "update": ""
        },
        {
            "path": "node_modules/suneditor/dist/css/suneditor.min.css",
            "update": ""
        },
        {
            "path": "lib/codemirror.min.css",
            "update": ""
        },
        {
            "path": "${build.out.css.path}",
            "bundle": true,
            "exclude": ["fashion"]
        }
    ],

    "loader": {
        "cache": false,
        "cacheParam": "_dc"
    },

    "production": {
        "output": {
            "appCache": {
                "enable": false,
                "path": "cache.appcache"
            }
        },
        "loader": {
            "cache": "${build.timestamp}"
        },
        "cache": {
            "enable": true
        },
        "compressor": {
            "type": "closure",
            "compression": "simple"
        }
    },

    "testing": {},

    "development": {
        "watch": {
            "delay": 250
        }
    },

    "bootstrap": {
        "base": "${app.dir}",

        "manifest": "${build.id}.json",

        "microloader": "bootstrap.js",
        "css": "bootstrap.css"
    },

    "output": {
        "base": "${workspace.build.dir}/${build.environment}/${app.name}",
        "page": "index.html",
        "manifest": "${build.id}.json",
        "js": "${build.id}/app.js",
        "appCache": {
            "enable": false
        },
        "resources": {
            "path": "${build.id}/resources",
            "shared": "resources"
        }
    },

    "cache": {
        "enable": false,
        "deltas": "${build.id}/deltas"
    },

    "appCache": {
        "cache": [
            "index.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },

    "resources": [
        {
            "path": "resources",
            "output": "shared"
        },
        {
            "path": "${toolkit.name}/resources"
        },
        {
            "path": "${build.id}/resources"
        }
    ],

    "archivePath": "archive/${build.id}",

    "builds": {
        "classic": {
            "toolkit": "classic",
            "theme": "theme-neptune",
            "sass": {
                "generated": {
                    "var": "classic/sass/save.scss",
                    "src": "classic/sass/save"
                }
            }
        },

       "modern": {
            "toolkit": "modern",
            "theme": "theme-material",
            "sass": {
                "generated": {
                    "var": "modern/sass/save.scss",
                    "src": "modern/sass/save"
                }
            }
        }
    },

    "ignore": [
        "(^|/)CVS(/?$|/.*?$)"
    ],

    "id": "4247d1ac-80d7-4c34-93b7-4386bb28d95c"
}
